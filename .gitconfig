[user]
	name = Jeff Hykin
	email = jeff.hykin@gmail.com
	signingkey = /Users/jeffhykin/.ssh/git_signing.pub
[pull]
	rebase = false
[color]
	ui = true
[push]
	default = upstream
[core]
	editor = nano
[safe]
	directory = /Users/jeffhykin/repos/nix-ros-overlay-generated
[advice]
	detachedHead = false
[diff]
	tool = vscode
[difftool "vscode"]
	cmd = "code --wait --diff  "
[gpg]
	program = gpg
	format = ssh
[commit]
	gpgsign = true
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
